{
  "version": 2,
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/api"
    }
  ],
  "crons": [{
    "path": "/api/cron",
    "schedule": "0 * * * *"
  }],
    "headers": [
      {
        "source": "/(.*)",
        "headers": [
          {
            "key": "ACCESS-CONTROL-ALLOW-ORIGIN",
            "value": "*"
          }
        ]
      }
    ]
}